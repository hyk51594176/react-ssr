{
  "name": "react-ssr",
  "version": "0.0.0",
  "scripts": {
    "dev": "nodemon --watch server.ts --exec 'ts-node server.ts'",
    "build": "npm run lint && npm run build:client && npm run build:server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx",
    "serve": "npm run build && cross-env NODE_ENV=production ts-node server.ts",
    "tsc": "tsc --noEmit",
    "lint": "npm run tsc && npm run lint:script && npm run lint:style",
    "prerender": "vite build --outDir dist/static && npm run build:server && ts-node prerender",
    "lint:script": "eslint --ext .tsx,.ts,.js,.jsx ./src ./prerender.ts ./server.ts",
    "lint-fix:script": "npm run lint:script -- --fix",
    "lint:style": "stylelint \"src/**/*.less\" ",
    "lint-fix:style": "npm run lint:style -- --fix"
  },
  "dependencies": {
    "@hanyk/redux-model": "^0.1.2",
    "axios": "^1.3.4",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.2",
    "redux": "^4.2.1",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "colors": "^1.4.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "less": "^4.1.3",
    "nodemon": "^2.0.21",
    "postcss": "^8.4.21",
    "postcss-less": "^6.0.0",
    "postcss-preset-env": "^8.0.1",
    "prettier": "^2.8.4",
    "stylelint": "^15.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.7.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  }
}